  },
  "revealjs": {
    "dependencies": [],
    "devDependencies": ["decktape"],
    "resources": [{ "src": "docs/quarto/rjs", "dest": "_extensions/rjs" }],
    "postinstalls": "",
    "scripts": {
      "postbuild": "decktape --size 960x720 .\\output\\index.html .\\output\\index.pdf"
    }
  }
}
