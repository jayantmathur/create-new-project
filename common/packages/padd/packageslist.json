{
  "web-ui": {
    "dependencies": [
      "daisyui",
      "react-spring",
      "framer-motion",
      "react-dropzone",
      "react-icons",
      "react-hook-form"
    ],
    "resources": [{ "src": "web/tailwind", "dest": "" }],
    "postinstalls": "",
    "scripts": {}
  },
  "client-auth": {
    "dependencies": ["@supabase/supabase-js", "@prisma/client"],
    "devDependencies": ["supabase", "prisma", "encoding"],
    "resources": [{ "src": "web/supabase", "dest": "supabase" }],
    "postinstalls": "",
    "scripts": {
      "update-supatypes": "supabase gen types typescript --schema public > supabase/types.ts"
    }
  },
  "app-deploy": {
    "dependencies": [
      "next-pwa",
      "@vercel/analytics",
      "@capacitor/core",
      "@capacitor/android",
      "@capacitor/ios"
    ],
    "devDependencies": [
      "vercel",
      "@capacitor/cli",
      "@capacitor/assets",
      "@tauri-apps/cli"
    ],
    "resources": [{ "src": "web/capacitor", "dest": "" }],
    "postinstalls": [
      "next telemetry disable",
      "npx cap init && npx cap add android && npx cap add ios && npx cap sync",
      "npx icon-gen --input public/icon.svg --output public --favicon --favicon-png-sizes 1024",
      "yarn @capacitor/assets generate --assetPath public --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#191919' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#191919'",
      "npx icon-gen --input public/icon.svg --output icons --ico --ico-name icon --icns --icns-name icon",
      "yarn tauri init && yarn tauri icon icons/icon.ico"
    ],
    "scripts": {
      "deploy": "vercel --prod",
      "taurize": "yarn tauri build"
    }
  },
  "api": {
    "dependencies": ["axios", "swr"],
    "devDependencies": [],
    "resources": [],
    "postinstalls": "",
    "scripts": {}
  },
  "mdx": {
    "dependencies": [
      "@next/mdx",
      "@mdx-js/loader",
      "@mdx-js/react",
      "next-mdx-remote",
      "gray-matter",
      "remark-gfm"
    ],
    "devDependencies": [],
    "resources": [],
    "postinstalls": "",
    "scripts": {}
  },
  "three": {
    "dependencies": [
      "@react-three/rapier",
      "@react-three/drei",
      "@react-three/fiber",
      "@react-three/xr",
      "r3f-perf",
      "@types/three",
      "three",
      "three-stdlib"
    ],
    "devDependencies": [],
    "extras?": [
      "@react-three/flex",
      "@react-three/csg",
